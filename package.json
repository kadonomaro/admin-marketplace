{
  "name": "admin-marketplace",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node server",
    "preview": "vite preview",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vue-tsc --noEmit && vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "start": "cross-env NODE_ENV=production node --experimental-json-modules server"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "express": "^4.18.1",
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.38",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/server-renderer": "^3.2.37",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^8.4.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  }
}
